{
  "devDependencies": {
    "@google/clasp": "^2.5.0",
    "prettier": "^3.1.0",
    "eslint": "^8.56.0"
  },
  "scripts": {
    "format": "prettier --write \"**/*.js\" \"**/*.json\"",
    "format:check": "prettier --check \"**/*.js\" \"**/*.json\"",
    "lint": "eslint \"**/*.js\"",
    "check": "npm run lint && npm run format:check"
  }
}
